{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "Topup response",
    "description": "Data odpovede Topup transakcie",
    "type": "object",
     "properties": {
        "uuid": {"$ref": "https://raw.githubusercontent.com/AXASOFT-POS/nucleus/main/jsonSchema/baseSchema.json#/$defs/uuidv7"},
        "termBaseRespData": {"$ref": "https://raw.githubusercontent.com/AXASOFT-POS/nucleus/main/jsonSchema/baseSchema.json#/$defs/TerminalBaseRespData"},
                "product": {
                    "type": "string",
                    "$comment": "Identifikator Topup produktu z POS"
                },
                "data1": {
                    "type": "string",
                    "$comment": "Zaklane data produktu"
                },
                "data2": {
                    "type": "string",
                    "$comment": "Doplnujuce data produktu"
                },
                "ean": {
                    "type": "string",
                    "$comment": "EAN kod produktu",
                    "pattern": "^[0-9]{13}$"
                }
   },
   "required": [
        "uuid",
        "product",
        "data1",
        "ean",
        "termBaseRespData"
    ]
}
