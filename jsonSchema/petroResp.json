{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "Petro response",
    "description": "Data odpovede petrol transakcie",
    "type": "object",
   "additionalProperties": false,
     "properties": {
        "uuid": {
            "type": "string",
            "$comment": "Jednoznacny identifikator transakcie uuid v7",
            "minLength": 36,
            "maxLength": 36,
            "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-7[0-9a-fA-F]{3}-[89ABab][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
        },
        "termBaseRespData": 
        {"$ref": "https://raw.githubusercontent.com/AXASOFT-POS/nucleus/main/jsonSchema/baseSchema.json#/$defs/TerminalBaseReqData"}
        },
    "$defs": {
         "PetroRespData": {
            "type": "object",
            "additionalProperties": false,
             "properties": {
                "retrRefNumber": {
                    "type": "string",
                    "minLength": 12,
                    "maxLength": 12,
                    "$comment": "P-37 Reference retrieval number"
                },
                "allowedProducts": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 999,
                    "$comment": "Dovolene produkty"
                },
                "scriptToCard": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 999,
                    "$comment": "P-55 Script do karty"
                }
            }
        }
    },
    "required": [
        "uuid",
        "termBaseRespData"
    ]
}
